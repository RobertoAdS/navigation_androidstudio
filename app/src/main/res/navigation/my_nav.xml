<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/tela1">
   <fragment
       android:id="@+id/tela1"
       android:name="com.example.myapplication.FirstFragment"
       android:label="fragment_first"
       tools:layout="@layout/fragment_first">
      <action
          android:id="@+id/action_tela1_to_secondFragment"
          app:destination="@id/secondFragment" />
      <action
          android:id="@+id/action_tela1_to_thirdFragment"
          app:destination="@id/thirdFragment" />
   </fragment>
   <fragment
       android:id="@+id/secondFragment"
       android:name="com.example.myapplication.SecondFragment"
       android:label="fragment_second"
       tools:layout="@layout/fragment_second" >
      <action
          android:id="@+id/action_secondFragment_to_fourFragment"
          app:destination="@id/fourFragment" />
   </fragment>
   <fragment
       android:id="@+id/thirdFragment"
       android:name="com.example.myapplication.ThirdFragment"
       android:label="ThirdFragment" >
      <action
          android:id="@+id/action_thirdFragment_to_fifthFragment"
          app:destination="@id/fifthFragment" />
   </fragment>
   <fragment
       android:id="@+id/fifthFragment"
       android:name="com.example.myapplication.FifthFragment"
       android:label="FifthFragment" >
      <action
          android:id="@+id/action_fifthFragment_to_sixFragment"
          app:destination="@id/sixFragment" />
   </fragment>
   <fragment
       android:id="@+id/fourFragment"
       android:name="com.example.myapplication.FourFragment"
       android:label="FourFragment" >
      <action
          android:id="@+id/action_fourFragment_to_sixFragment"
          app:destination="@id/sixFragment" />
   </fragment>
   <fragment
       android:id="@+id/sixFragment"
       android:name="com.example.myapplication.SixFragment"
       android:label="SixFragment" >
      <action
          android:id="@+id/action_sixFragment_to_tela1"
          app:destination="@id/tela1" />
   </fragment>

</navigation>
